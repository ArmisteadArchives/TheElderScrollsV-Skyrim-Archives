<p id="notice"><%= notice %></p>

<h1>Listing Ingredient Effects</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Effect</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ingredient_effects.each do |ingredient_effect| %>
      <tr>
        <td><%= ingredient_effect.name %></td>
        <td><%= ingredient_effect.effect %></td>
        <td><%= link_to 'Show', ingredient_effect %></td>
        <td><%= link_to 'Edit', edit_ingredient_effect_path(ingredient_effect) %></td>
        <td><%= link_to 'Destroy', ingredient_effect, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ingredient effect', new_ingredient_effect_path %>
